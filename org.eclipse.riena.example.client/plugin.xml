   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="fileMenu"
               label="File"
               mnemonic="F">
            <menu
                  label="Zoom"
                  mnemonic="Z">
               <command
                     commandId="org.eclipse.riena.example.client.zoomInCommand"
                     id="org.eclipse.riena.example.client.zoomInMenuItem"
                     mnemonic="I"
                     style="push">
               </command>
               <command
                     commandId="org.eclipse.riena.example.client.zoomOutCommand"
                     id="org.eclipse.riena.example.client.zoomOutMenuItem"
                     mnemonic="O"
                     style="push">
               </command>
            </menu>
            <command
                  commandId="org.eclipse.riena.example.client.exitCommand"
                  id="org.eclipse.riena.example.client.exitMenuItem"
                  mnemonic="x">
            </command>
         </menu>
         <!--
         <menu
               label="Preferences"
               mnemonic="P">
            <menu
                  label="Look &apos;n&apos; Feel"
                  mnemonic="L">
               <command
                     commandId="org.eclipse.riena.example.client.rienaDefaultLnfCommand"
                     id="org.eclipse.riena.example.client.rienaDefaultLnfMenuItem"
                     mnemonic="D">
               </command>
               <command
                     commandId="org.eclipse.riena.example.client.exampleLnfCommand"
                     id="org.eclipse.riena.example.client.exampleLnfMenuItem"
                     mnemonic="x">
               </command>
            </menu>
         </menu>
         -->
         <menu
               id="contextMenu"
               label="Context menu"
               mnemonic="x">
            <command
                  commandId="org.eclipse.riena.example.client.certainViewCommand"
                  icon="icons/ledred.png"
                  id="org.eclipse.riena.example.client.certainViewMenuItem"
                  mnemonic="V"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.riena.example.client.certainPerspectiveCommand"
                  icon="icons/ledlightgreen.png"
                  id="org.eclipse.riena.example.client.certainPerspectiveMenuItem"
                  mnemonic="P"
                  style="push">
            </command>
         </menu>
         <menu
               id="navigationMenu"
               label="Navigation menu"
               mnemonic="N">
            <command
                  commandId="org.eclipse.riena.example.client.histBackCommand"
                  icon="icons/backward.gif"
                  id="org.eclipse.riena.example.client.histBackMenuItem"
                  mnemonic="B"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.riena.example.client.histForCommand"
                  icon="icons/forward.gif"
                  id="org.eclipse.riena.example.client.histForMenuItem"
                  mnemonic="F"
                  style="push">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="org.eclipse.riena.example.client.toolbar">
            <command
                  commandId="org.eclipse.riena.example.client.exitCommand"
                  id="org.eclipse.riena.example.client.exitToolBarItem"
                  label="Exit"
                  mnemonic="x"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.riena.example.client.certainViewCommand"
                  icon="icons/ledred.png"
                  id="org.eclipse.riena.example.client.certainViewToolBarItem"
                  label="View"
                  mnemonic="V"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.riena.example.client.certainPerspectiveCommand"
                  icon="icons/ledlightgreen.png"
                  label="Perspective"
                  mnemonic="P"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.riena.example.client.histBackCommand"
                  icon="icons/backward.gif"
                  label="History Back"
                  mnemonic="B"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.riena.example.client.histForCommand"
                  icon="icons/forward.gif"
                  label="History Forward"
                  mnemonic="F"
                  style="push">
            </command>
         </toolbar>
      </menuContribution>
