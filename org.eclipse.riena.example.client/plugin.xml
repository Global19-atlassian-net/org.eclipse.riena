            category="LogCollector"
            class="org.eclipse.riena.monitor.client.LogServiceCollector: collectRange=1,2; triggerRange=1,2"
            maxItems="50">
      </collector>
      <collector
            category="CustomCollector"
            class="org.eclipse.riena.monitor.client.LogServiceCollector: collectRange=-2..0; triggerRange=-2"
            maxItems="250">
      </collector>
   </extension>
   
   <extension point="org.eclipse.riena.monitor.store">
