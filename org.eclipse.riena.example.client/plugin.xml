            filterId="rienaExample.offline">
         <nodeId
               id="application">
         </nodeId>
         <ruleMarkerNavigation
               marker="hidden"
               nodeId="*/org.eclipse.riena.example.uiProcesses">
         </ruleMarkerNavigation>
         <ruleMarkerNavigation
               marker="hidden"
               nodeId="*/org.eclipse.riena.example.navigation*">
         </ruleMarkerNavigation>
         <ruleMarkerRidget
               marker="hidden"
               ridgetId="*/textPassword">
         </ruleMarkerRidget>
         <ruleMarkerRidget
               marker="hidden"
               ridgetId="*/ui_label2">
         </ruleMarkerRidget>
         <ruleMarkerRidget
               marker="output"
               ridgetId="*/textField">
         </ruleMarkerRidget>
         <ruleValidatorRidget
               ridgetId="*/ui_text1"
               validationTime="onUIControlEdit"
               validator="org.eclipse.riena.ui.ridgets.validation.MaxLength:12">
         </ruleValidatorRidget>
         <ruleMarkerMenuItem
               itemId="org.eclipse.riena.example.client.exitToolBarItem"
               marker="disabled">
         </ruleMarkerMenuItem>
      </filter>
      <filter
            filterId="rienaExample.menuToolBar">
         <nodeId
               id="/application">
         </nodeId>
         <ruleMarkerMenuItem
               itemId="org.eclipse.riena.example.client.zoom*MenuItem"
               marker="disabled">
         </ruleMarkerMenuItem>
         <ruleMarkerMenuItem
               itemId="org.eclipse.riena.example.client.exitMenuItem"
               marker="disabled">
         </ruleMarkerMenuItem>
         <ruleMarkerMenuItem
               itemId="org.eclipse.riena.example.client.exitToolBarItem"
               marker="disabled">
         </ruleMarkerMenuItem>
         <ruleMarkerMenuItem
               itemId="navigationMenu"
               marker="hidden">
         </ruleMarkerMenuItem>
      </filter>
      <filter
            filterId="rienaExample.ridget">
         <nodeId
               id="/application">
         </nodeId>
         <ruleMarkerRidget
               marker="hidden"
               ridgetId="/application/*/org.eclipse.riena.example.text/textModel?">
         </ruleMarkerRidget>
         <ruleValidatorRidget
               ridgetId="/application/*/org.eclipse.riena.example.text/*"
               validationTime="onUIControlEdit"
               validator="org.eclipse.riena.ui.ridgets.validation.MaxLength:12">
         </ruleValidatorRidget>
      </filter>
      <filter
            filterId="rienaExample.navigation">
         <nodeId
               id="/application">
         </nodeId>
         <ruleMarkerNavigation
               marker="hidden"
               nodeId="/application/*/org.eclipse.riena.example.sharedViews">
         </ruleMarkerNavigation>
         <ruleMarkerNavigation
               marker="hidden"
               nodeId="/application/*/org.eclipse.riena.example.uiProcesses">
         </ruleMarkerNavigation>
         <ruleMarkerNavigation
               marker="disabled"
               nodeId="/application/*/logcollectorModule">
         </ruleMarkerNavigation>
         <ruleMarkerNavigation
               marker="disabled"
               nodeId="/application/*/org.eclipse.riena.example.tree">
         </ruleMarkerNavigation>
         <ruleMarkerNavigation
               marker="disabled"
               nodeId="/application/*/org.eclipse.riena.example.treeTable">
         </ruleMarkerNavigation>
         <ruleMarkerNavigation
               marker="hidden"
               nodeId="/application/*/org.eclipse.riena.example.table">
         </ruleMarkerNavigation>
         <ruleMarkerNavigation
               marker="disabled"
               nodeId="/application/*/org.eclipse.riena.example.filters/*/org.eclipse.riena.example.filterridget">
         </ruleMarkerNavigation>
         <ruleMarkerNavigation
               marker="disabled"
               nodeId="/application/*/org.eclipse.riena.example.filters/*/org.eclipse.riena.example.filternavigation">
         </ruleMarkerNavigation>
         <ruleMarkerNavigation
               marker="hidden"
               nodeId="/application/*/org.eclipse.riena.example.filters/*/org.eclipse.riena.example.filteraction">
         </ruleMarkerNavigation>
         <ruleMarkerNavigation
               marker="hidden"
               nodeId="/application/*/org.eclipse.riena.example.filters/*/org.eclipse.riena.example.filterdefinition">
         </ruleMarkerNavigation>
      </filter>
      <filter
            filterId="rienaExample.ridget01">
         <nodeId
               id="/application">
         </nodeId>
         <ruleValidatorRidget
               ridgetId="/application/*/org.eclipse.riena.example.filters/*/org.eclipse.riena.example.filterexample/sampleText"
               validationTime="onUpdateToModel"
               validator="org.eclipse.riena.ui.ridgets.validation.ValidCharacters:01">
         </ruleValidatorRidget>
      </filter>
      <filter
            filterId="rienaExample.ridgetDisable">
         <nodeId
               id="/application">
         </nodeId>
         <ruleMarkerRidget
               marker="disabled"
               ridgetId="/application/*/org.eclipse.riena.example.filters/*/org.eclipse.riena.example.filterexample/sample*">
         </ruleMarkerRidget>
      </filter>
      <filter
            filterId="rienaExample.ridgetHide">
         <nodeId
               id="/application">
         </nodeId>
         <ruleMarkerRidget
               marker="hidden"
               ridgetId="/application/*/org.eclipse.riena.example.filters/*/org.eclipse.riena.example.filterexample/sample*">
         </ruleMarkerRidget>
      </filter>
   </extension>
   
   <!-- To activate client monitoring for the example client simply uncomment this. -->
