   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.eclipse.riena.example.client.handler.ExitHandler"
            commandId="org.eclipse.riena.example.client.exitCommand">
      </handler>
      <handler
            class="org.eclipse.riena.example.client.handler.CertainViewHandler"
            commandId="org.eclipse.riena.example.client.certainViewCommand">
         <enabledWhen>
            <with
                  variable="activePartId">
               <equals
                     value="org.eclipse.riena.example.client.views.ComboSubModuleView">
               </equals>
            </with>
         </enabledWhen>
      </handler>
      <handler
            class="org.eclipse.riena.example.client.handler.CertainPerspectiveHandler"
            commandId="org.eclipse.riena.example.client.certainPerspectiveCommand">
         <enabledWhen>
            <with
                  variable="activeWorkbenchWindow.activePerspective">
               <equals
                     value="subapplication.2">
               </equals>
            </with>
         </enabledWhen>
      </handler>
      <handler
            class="org.eclipse.riena.example.client.handler.ZoomInHandler"
            commandId="org.eclipse.riena.example.client.zoomInCommand">
      </handler>
      <handler
            class="org.eclipse.riena.example.client.handler.ZoomOutHandler"
            commandId="org.eclipse.riena.example.client.zoomOutCommand">
      </handler>
      <handler
            class="org.eclipse.riena.example.client.handler.HistoryBackwardHandler"
            commandId="org.eclipse.riena.example.client.histBackCommand">
      </handler>
      <handler
            class="org.eclipse.riena.example.client.handler.HistoryForwardHandler"
            commandId="org.eclipse.riena.example.client.histForCommand">
      </handler>
   </extension>
