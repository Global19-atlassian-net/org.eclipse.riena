         -->
         <menu
               id="contextMenu"
               label="Context menu"
               mnemonic="x">
            <command
                  commandId="org.eclipse.riena.example.client.certainViewCommand"
                  icon="icons/ledred.png"
                  id="org.eclipse.riena.example.client.certainViewMenuItem"
                  mnemonic="V"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.riena.example.client.certainPerspectiveCommand"
                  icon="icons/ledlightgreen.png"
                  id="org.eclipse.riena.example.client.certainPerspectiveMenuItem"
                  mnemonic="P"
                  style="push">
            </command>
         </menu>
         <menu
               id="navigationMenu"
               label="Navigation menu"
               mnemonic="N">
            <command
                  commandId="org.eclipse.riena.example.client.histBackCommand"
                  icon="icons/backward.gif"
                  id="org.eclipse.riena.example.client.histBackMenuItem"
                  mnemonic="B"
