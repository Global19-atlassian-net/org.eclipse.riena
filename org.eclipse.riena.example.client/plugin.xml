<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>   
   <extension
         id="org.eclipse.riena.example.client.application"
         name="org.eclipse.riena.example.client.application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="org.eclipse.riena.example.client.application.SwtExampleApplication">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.eclipse.riena.navigation.ui.swt.views.SubApplicationPerspectiveFactory"
            id="subapplication.1"
            name="subapplication.1">
      </perspective>
      <perspective
            class="org.eclipse.riena.navigation.ui.swt.views.SubApplicationPerspectiveFactory"
            id="subapplication.2"
            name="subapplication.2">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="true"
            class="org.eclipse.riena.example.client.views.CustomerDetailView"
            id="customerDetailView"
            name="customerDetailView">
      </view>
      
            <view
            allowMultiple="true"
            class="org.eclipse.riena.example.client.views.SharedViewDemoView"
            id="sharedviewdemoid"
            name="sharedviewdemoid">
      </view>
      <view
            allowMultiple="true"
            class="org.eclipse.riena.example.client.views.RidgetsSubModuleView"
            id="RidgetsSubModuleView"
            name="RidgetsSubModuleView">
      </view>
      <view
            allowMultiple="true"
            class="org.eclipse.riena.example.client.views.UiProcessDemoView"
            id="org.eclipse.riena.sample.app.client.uiprocessdemoview"
            name="org.eclipse.riena.sample.app.client.uiprocessdemoview">
      </view>
      <view
            allowMultiple="true"
            class="org.eclipse.riena.example.client.views.ComboView"
            id="org.eclipse.riena.example.client.views.ComboView"
            name="org.eclipse.riena.example.client.views.ComboView">
      </view>
      <view
            allowMultiple="true"
            class="org.eclipse.riena.example.client.views.ListView"
            id="org.eclipse.riena.example.client.views.ListView"
            name="org.eclipse.riena.example.client.views.ListView">
      </view>
      <view
            allowMultiple="true"
            class="org.eclipse.riena.example.client.views.TextView"
            id="org.eclipse.riena.example.client.views.TextView"
            name="org.eclipse.riena.example.client.views.TextView">
      </view>
      <view
            allowMultiple="true"
            class="org.eclipse.riena.example.client.views.MarkerView"
            id="org.eclipse.riena.example.client.views.MarkerView"
            name="org.eclipse.riena.example.client.views.MarkerView">
      </view>
      <view
            allowMultiple="true"
            class="org.eclipse.riena.example.client.views.FocusableView"
            id="org.eclipse.riena.example.client.views.FocusableView"
            name="org.eclipse.riena.example.client.views.FocusableView">
      </view>
      <view
            allowMultiple="true"
            class="org.eclipse.riena.example.client.views.ValidationView"
            id="org.eclipse.riena.example.client.views.ValidationView"
            name="org.eclipse.riena.example.client.views.ValidationView">
      </view>
      <view
            allowMultiple="true"
            class="org.eclipse.riena.example.client.views.SystemPropertiesView"
            id="org.eclipse.riena.example.client.views.SystemPropertiesView"
            name="org.eclipse.riena.example.client.views.SystemPropertiesView">
      </view>
      <view
            allowMultiple="true"
            class="org.eclipse.riena.example.client.views.TreeView"
            id="org.eclipse.riena.example.client.views.TreeView"
            name="org.eclipse.riena.example.client.views.TreeView">
      </view>
      <view
            allowMultiple="true"
            class="org.eclipse.riena.example.client.views.TreeTableView"
            id="org.eclipse.riena.example.client.views.TreeTableView"
            name="org.eclipse.riena.example.client.views.TreeTableView">
      </view>
      <view
            allowMultiple="true"
            class="org.eclipse.riena.example.client.views.TableView"
            id="org.eclipse.riena.example.client.views.TableView"
            name="org.eclipse.riena.example.client.views.TableView">
      </view>
      <view
            allowMultiple="true"
            class="org.eclipse.riena.example.client.views.NavigationSubModuleView"
            id="org.eclipse.riena.example.client.views.NavigationSubModuleView"
            name="org.eclipse.riena.example.client.views.NavigationSubModuleView">
      </view>
      <view
            allowMultiple="true"
            class="org.eclipse.riena.example.client.views.StatuslineSubModuleView"
            id="org.eclipse.riena.example.client.views.StatuslineSubModuleView"
            name="org.eclipse.riena.example.client.views.StatuslineSubModuleView">
      </view>
      <view
            allowMultiple="true"
            class="org.eclipse.riena.example.client.views.NavigateSubModuleView"
            id="org.eclipse.riena.example.client.views.NavigateSubModuleView"
            name="org.eclipse.riena.example.client.views.NavigateSubModuleView">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="subapplication.1">
         <actionSet
               id="org.eclipse.riena.example.client.actionSet">
         </actionSet>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="org.eclipse.riena.example.client.exitCommand"
            name="Exit">
      </command>
      <command
            id="org.eclipse.riena.example.client.zoomInCommand"
            name="In">
      </command>
      <command
            id="org.eclipse.riena.example.client.certainViewCommand"
            name="Certain View">
      </command>
      <command
            id="org.eclipse.riena.example.client.zoomOutCommand"
            name="Out">
      </command>
      <command
            id="org.eclipse.riena.example.client.certainPerspectiveCommand"
            name="Certain Perspective">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="fileMenu"
               label="File"
               mnemonic="F">
            <menu
                  label="Zoom"
                  mnemonic="Z">
               <command
                     commandId="org.eclipse.riena.example.client.zoomInCommand"
                     id="org.eclipse.riena.example.client.zoomInMenuItem"
                     mnemonic="I"
                     style="push">
               </command>
               <command
                     commandId="org.eclipse.riena.example.client.zoomOutCommand"
                     id="org.eclipse.riena.example.client.zoomOutMenuItem"
                     mnemonic="O"
                     style="push">
               </command>
            </menu>
            <command
                  commandId="org.eclipse.riena.example.client.exitCommand"
                  id="org.eclipse.riena.example.client.exitMenuItem"
                  mnemonic="x">
            </command>
         </menu>
         <!--
         <menu
               label="Preferences"
               mnemonic="P">
            <menu
                  label="Look &apos;n&apos; Feel"
                  mnemonic="L">
               <command
                     commandId="org.eclipse.riena.example.client.rienaDefaultLnfCommand"
                     id="org.eclipse.riena.example.client.rienaDefaultLnfMenuItem"
                     mnemonic="D">
               </command>
               <command
                     commandId="org.eclipse.riena.example.client.exampleLnfCommand"
                     id="org.eclipse.riena.example.client.exampleLnfMenuItem"
                     mnemonic="x">
               </command>
            </menu>
         </menu>
         -->
         <menu
               id="contextMenu"
               label="Context menu"
               mnemonic="x">
            <command
                  commandId="org.eclipse.riena.example.client.certainViewCommand"
                  icon="icons/ledred.png"
                  id="org.eclipse.riena.example.client.certainViewMenuItem"
                  mnemonic="V"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.riena.example.client.certainPerspectiveCommand"
                  icon="icons/ledlightgreen.png"
                  id="org.eclipse.riena.example.client.certainPerspectiveMenuItem"
                  mnemonic="P"
                  style="push">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="org.eclipse.riena.example.client.toolbar">
            <command
                  commandId="org.eclipse.riena.example.client.exitCommand"
                  id="org.eclipse.riena.example.client.exitToolBarItem"
                  label="Exit"
                  mnemonic="x"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.riena.example.client.certainViewCommand"
                  icon="icons/ledred.png"
                  id="org.eclipse.riena.example.client.certainViewToolBarItem"
                  label="View"
                  mnemonic="V"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.riena.example.client.certainPerspectiveCommand"
                  icon="icons/ledlightgreen.png"
                  label="Perspective"
                  mnemonic="P"
                  style="push">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.eclipse.riena.example.client.handler.ExitHandler"
            commandId="org.eclipse.riena.example.client.exitCommand">
      </handler>
      <handler
            class="org.eclipse.riena.example.client.handler.CertainViewHandler"
            commandId="org.eclipse.riena.example.client.certainViewCommand">
         <enabledWhen>
            <with
                  variable="activePartId">
               <equals
                     value="customerDetailView">
               </equals>
            </with>
         </enabledWhen>
      </handler>
      <handler
            class="org.eclipse.riena.example.client.handler.CertainPerspectiveHandler"
            commandId="org.eclipse.riena.example.client.certainPerspectiveCommand">
         <enabledWhen>
            <with
                  variable="activeWorkbenchWindow.activePerspective">
               <equals
                     value="subapplication.2">
               </equals>
            </with>
         </enabledWhen>
      </handler>
      <handler
            class="org.eclipse.riena.example.client.handler.ZoomInHandler"
            commandId="org.eclipse.riena.example.client.zoomInCommand">
      </handler>
      <handler
            class="org.eclipse.riena.example.client.handler.ZoomOutHandler"
            commandId="org.eclipse.riena.example.client.zoomOutCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.riena.ui.swt.lnfrenderer">
      <renderer
            class="org.eclipse.riena.example.client.application.ExampleModuleGroupRenderer"
            lnfid="ExampleLnf"
            lnfkey="ModuleGroup.renderer">
      </renderer>
   </extension>
   
   <extension
         point="org.eclipse.riena.navigation.NavigationNodePresentation">
      <navigationNodePresentation
            presentationId="open2"
            nodeBuilder="org.eclipse.riena.example.client.navigation.model.Open2NavigationNodeBuilder"
            parentPresentationId="app1">
      </navigationNodePresentation>
   </extension>
   <extension
         point="org.eclipse.riena.navigation.NavigationNodePresentation">
      <navigationNodePresentation
            presentationId="open3"
            nodeBuilder="org.eclipse.riena.example.client.navigation.model.Open3NavigationNodeBuilder"
            parentPresentationId="playground">
      </navigationNodePresentation>
   </extension>
   
</plugin>
