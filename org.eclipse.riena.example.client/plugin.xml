   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="fileMenu"
               label="File"
               mnemonic="F">
            <menu
                  id="org.eclipse.riena.example.client.zoomMenu"
                  label="Zoom"
                  mnemonic="Z">
               <command
                     commandId="org.eclipse.riena.example.client.zoomInCommand"
                     id="org.eclipse.riena.example.client.zoomInMenuItem"
                     mnemonic="I"
                     style="push">
               </command>
               <command
                     commandId="org.eclipse.riena.example.client.zoomOutCommand"
                     id="org.eclipse.riena.example.client.zoomOutMenuItem"
                     mnemonic="O"
                     style="push">
               </command>
            </menu>
            <command
                  commandId="org.eclipse.riena.example.client.exitCommand"
                  id="org.eclipse.riena.example.client.exitMenuItem"
                  mnemonic="x">
            </command>
         </menu>
         <!--
         <menu
               label="Preferences"
               mnemonic="P">
            <menu
                  label="Look &apos;n&apos; Feel"
                  mnemonic="L">
               <command
