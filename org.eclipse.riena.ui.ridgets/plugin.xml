<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="org.eclipse.riena.ui.ridgets.correspondinglabel.config" name="Corresponding Label Config" schema="schema/org.eclipse.riena.ui.ridgets.correspondinglabel.config.exsd"/>
   <extension-point id="correspondingLabel" name="Corresponding Label Configuration" schema="schema/correspondingLabel.exsd"/>
   <extension-point id="org.eclipse.riena.ui.ridgets.labelfinderstrategy" name="Corresponding Label Strategy" schema="schema/org.eclipse.riena.ui.ridgets.correspondinglabel.strategy.exsd"/>
   <extension-point id="labelFinderStrategy" name="Corresponding Label Strategy" schema="schema/labelFinderStrategy.exsd"/>
   <extension-point id="annotationHandler" name="Annotation Handler for Ridget Containers" schema="schema/annotationHandler.exsd"/>

    <extension
         point="org.eclipse.riena.ui.ridgets.annotationHandler">
      <annotationHandler
            annotation="org.eclipse.riena.ui.ridgets.annotation.HandlesActionCallback"
            handler="org.eclipse.riena.ui.ridgets.annotation.handler.ActionCallbackAnnotationHandler">
      </annotationHandler>
      <annotationHandler
            annotation="org.eclipse.riena.ui.ridgets.annotation.HandlesSelectionChange"
            handler="org.eclipse.riena.ui.ridgets.annotation.handler.SelectionChangedAnnotationHandler">
      </annotationHandler>
      <annotationHandler
            annotation="org.eclipse.riena.ui.ridgets.annotation.HandlesFocusGained"
            handler="org.eclipse.riena.ui.ridgets.annotation.handler.FocusGainedAnnotationHandler">
      </annotationHandler>
      <annotationHandler
            annotation="org.eclipse.riena.ui.ridgets.annotation.HandlesFocusLost"
            handler="org.eclipse.riena.ui.ridgets.annotation.handler.FocusLostAnnotationHandler">
      </annotationHandler>
      <annotationHandler
            annotation="org.eclipse.riena.ui.ridgets.annotation.HandlesPropertyChange"
            handler="org.eclipse.riena.ui.ridgets.annotation.handler.PropertyChangeAnnotationHandler">
      </annotationHandler>
      <annotationHandler
            annotation="org.eclipse.riena.ui.ridgets.annotation.HandlesDoubleClick"
            handler="org.eclipse.riena.ui.ridgets.annotation.handler.DoubleClickAnnotationHandler">
      </annotationHandler>
   </extension>
   
</plugin>
