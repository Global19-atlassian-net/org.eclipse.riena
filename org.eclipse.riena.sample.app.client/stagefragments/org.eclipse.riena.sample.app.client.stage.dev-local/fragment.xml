<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<fragment>

	<extension
		point="org.eclipse.core.variables.valueVariables">
		<variable
			description="Riena stage name"
			name="riena.stage"
			readOnly="true"
			initialValue="develop local"/>
		<variable
			description="Name of the host to connect to"
			name="riena.hostname"
			readOnly="true"
			initialValue="localhost:8080"/>
	</extension>

</fragment>
