<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="lnfrenderer" name="Look and Feel renderer" schema="schema/lnfrenderer.exsd"/>
   <extension-point id="imagepath" name="Image Path" schema="schema/imagepath.exsd"/>
   <extension
         point="org.eclipse.riena.ui.core.uiSynchronizer">
		<uiSynchronizer class="org.eclipse.riena.ui.swt.uiprocess.SwtUISynchronizer"/>         
   </extension>
   <extension
         point="org.eclipse.riena.ui.swt.lnfrenderer">
      <renderer
            class="org.eclipse.riena.ui.swt.lnf.renderer.DialogBorderRenderer"
            lnfkey="Dialog.borderRenderer">
      </renderer>
      <renderer
            class="org.eclipse.riena.ui.swt.lnf.renderer.DialogTitleBarRenderer"
            lnfkey="Dialog.renderer">
      </renderer>
   </extension>
   <extension
         point="org.eclipse.riena.ui.swt.imagepath">
      <path
            path="icons">
      </path>
   </extension>
</plugin>
