<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="org.eclipse.riena.core.logging.listeners" name="org.eclipse.riena.core.logging.listeners" schema="schema/org.eclipse.riena.core.logging.listeners.exsd"/>
   <extension-point id="org.eclipse.riena.core.logging.catchers" name="org.eclipse.riena.core.logging.catchers" schema="schema/org.eclipse.riena.core.logging.catchers.exsd"/>
   <extension
         point="org.eclipse.riena.core.logging.listeners">
         <!-- This is examplary!
         <logListener name="Syso" listener-class="org.eclipse.riena.core.logging.SysoLogListener" sync="true"/>
         <logListener name="Log4j" listener-class="org.eclipse.riena.core.logging.Log4jLogListener" sync="true"/>
         -->
   </extension>
   <extension
         point="org.eclipse.riena.core.logging.catchers">
         <logCatcher name="Platform" class="org.eclipse.riena.core.logging.PlatformLogCatcher"/>
   </extension>
</plugin>
