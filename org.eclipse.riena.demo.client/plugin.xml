<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin><extension
         id="org.eclipse.riena.demo.client.application"
         name="org.eclipse.riena.demo.client.application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="org.eclipse.riena.demo.client.customer.application.SwtDemoApplication">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.eclipse.riena.navigation.ui.swt.views.SubApplicationView"
            id="customer"
            name="Customer">
      </perspective>
      <perspective
            class="org.eclipse.riena.navigation.ui.swt.views.SubApplicationView"
            id="order"
            name="Order">
      </perspective>
      <perspective
            class="org.eclipse.riena.navigation.ui.swt.views.SubApplicationView"
            id="pim"
            name="Mail">
      </perspective>
   </extension>
   <extension point="org.eclipse.ui.views">
      <view
            allowMultiple="true"
            class="org.eclipse.riena.demo.client.customer.views.CustomerSearchView"
            id="org.eclipse.riena.demo.kundensuche"
            name="org.eclipse.riena.demo.client.customer.views.KundenSucheView"
            restorable="true">
      </view>
      <view
            allowMultiple="true"
            class="org.eclipse.riena.demo.client.customer.views.CustomerAddressView"
            id="org.eclipse.riena.demo.customerRecordAddress"
            name="customerRecordAddress"
            restorable="true">
      </view>
      <view
            allowMultiple="true"
            class="org.eclipse.riena.demo.client.customer.views.CustomerContractView"
            id="org.eclipse.riena.demo.customerRecordContract"
            name="customerRecordContract"
            restorable="true">
      </view>
      <view
            allowMultiple="true"
            class="org.eclipse.riena.demo.client.customer.views.CustomerOverview"
            id="org.eclipse.riena.demo.customerRecordOverview"
            name="customerRecordOverview"
            restorable="true">
      </view>
      <view
            allowMultiple="true"
            class="org.eclipse.riena.demo.client.customer.views.CustomerPersonalDataView"
            id="org.eclipse.riena.demo.customerRecordPersonalData"
            name="customerRecordPersonalData"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="org.eclipse.riena.demo.client.exitCommand"
            name="Exit">
      </command>
      <command
            id="org.eclipse.riena.demo.client.zoomInCommand"
            name="In">
      </command>
      <command
            id="org.eclipse.riena.demo.client.certainViewCommand"
            name="Certain View (Combo)">
      </command>
      <command
            id="org.eclipse.riena.demo.client.zoomOutCommand"
            name="Out">
      </command>
      <command
            id="org.eclipse.riena.demo.client.certainPerspectiveCommand"
            name="Certain Perspective (Playground)">
      </command>
            <command
            id="org.eclipse.riena.demo.client.histBackCommand"
            name="History Backward">
      </command>
      <command
            id="org.eclipse.riena.demo.client.histForCommand"
            name="History Forward">
      </command>
    </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="fileMenu"
               label="File"
               mnemonic="F">
            <menu
                  id="org.eclipse.riena.demo.client.zoomMenu"
                  label="Zoom"
                  mnemonic="Z">
               <command
                     commandId="org.eclipse.riena.demo.client.zoomInCommand"
                     id="org.eclipse.riena.demo.client.zoomInMenuItem"
                     mnemonic="I"
                     style="push">
               </command>
               <command
                     commandId="org.eclipse.riena.demo.client.zoomOutCommand"
                     id="org.eclipse.riena.demo.client.zoomOutMenuItem"
                     mnemonic="O"
                     style="push">
               </command>
            </menu>
            <command
                  commandId="org.eclipse.riena.demo.client.exitCommand"
                  id="org.eclipse.riena.demo.client.exitMenuItem"
                  mnemonic="x">
            </command>
         </menu>
         <!--
         <menu
               label="Preferences"
               mnemonic="P">
            <menu
                  label="Look &apos;n&apos; Feel"
                  mnemonic="L">
               <command
                     commandId="org.eclipse.riena.example.client.rienaDefaultLnfCommand"
                     id="org.eclipse.riena.example.client.rienaDefaultLnfMenuItem"
                     mnemonic="D">
               </command>
               <command
                     commandId="org.eclipse.riena.example.client.exampleLnfCommand"
                     id="org.eclipse.riena.example.client.exampleLnfMenuItem"
                     mnemonic="x">
               </command>
            </menu>
         </menu>
         -->
         <menu
               id="contextMenu"
               label="Context menu"
               mnemonic="x">
            <command
                  commandId="org.eclipse.riena.demo.client.certainViewCommand"
                  icon="icons/ledred.png"
                  id="org.eclipse.riena.demo.client.certainViewMenuItem"
                  mnemonic="V"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.riena.demo.client.certainPerspectiveCommand"
                  icon="icons/ledlightgreen.png"
                  id="org.eclipse.riena.demo.client.certainPerspectiveMenuItem"
                  mnemonic="P"
                  style="push">
            </command>
         </menu>
         <menu
               id="navigationMenu"
               label="Navigation menu"
               mnemonic="N">
            <command
                  commandId="org.eclipse.riena.demo.client.histBackCommand"
                  icon="icons/backward.gif"
                  id="org.eclipse.riena.demo.client.histBackMenuItem"
                  mnemonic="B"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.riena.demo.client.histForCommand"
                  icon="icons/forward.gif"
                  id="org.eclipse.riena.demo.client.histForMenuItem"
                  mnemonic="F"
                  style="push">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="org.eclipse.riena.demo.client.toolbar">
            <command
                  commandId="org.eclipse.riena.demo.client.exitCommand"
                  id="org.eclipse.riena.demo.client.exitToolBarItem"
                  label="Exit"
                  mnemonic="x"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.riena.demo.client.certainViewCommand"
                  icon="icons/ledred.png"
                  id="org.eclipse.riena.demo.client.certainViewToolBarItem"
                  label="View"
                  mnemonic="V"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.riena.demo.client.certainPerspectiveCommand"
                  icon="icons/ledlightgreen.png"
                  label="Perspective"
                  mnemonic="P"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.riena.demo.client.histBackCommand"
                  icon="icons/backward.gif"
                  label="History Back"
                  mnemonic="B"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.riena.demo.client.histForCommand"
                  icon="icons/forward.gif"
                  label="History Forward"
                  mnemonic="F"
                  style="push">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.eclipse.riena.demo.client.handler.ExitHandler"
            commandId="org.eclipse.riena.demo.client.exitCommand">
      </handler>
      <handler
            class="org.eclipse.riena.demo.client.handler.CertainViewHandler"
            commandId="org.eclipse.riena.demo.client.certainViewCommand">
         <enabledWhen>
            <with
                  variable="activePartId">
               <equals
                     value="org.eclipse.riena.demo.client.views.ComboSubModuleView">
               </equals>
            </with>
         </enabledWhen>
      </handler>
      <handler
            class="org.eclipse.riena.demo.client.handler.CertainPerspectiveHandler"
            commandId="org.eclipse.riena.demo.client.certainPerspectiveCommand">
         <enabledWhen>
            <with
                  variable="activeWorkbenchWindow.activePerspective">
               <equals
                     value="subapplication.2">
               </equals>
            </with>
         </enabledWhen>
      </handler>
      <handler
            class="org.eclipse.riena.demo.client.handler.ZoomInHandler"
            commandId="org.eclipse.riena.demo.client.zoomInCommand">
      </handler>
      <handler
            class="org.eclipse.riena.demo.client.handler.ZoomOutHandler"
            commandId="org.eclipse.riena.demo.client.zoomOutCommand">
      </handler>
      <handler
            class="org.eclipse.riena.demo.client.handler.HistoryBackwardHandler"
            commandId="org.eclipse.riena.demo.client.histBackCommand">
      </handler>
      <handler
            class="org.eclipse.riena.demo.client.handler.HistoryForwardHandler"
            commandId="org.eclipse.riena.demo.client.histForCommand">
      </handler>
   </extension>
     <extension
         point="org.eclipse.riena.navigation.assemblies">
         <assembly
               id="org.eclipse.riena.demo.client.customer.customer.assembly"
               name="customer"
               autostartsequence="100"
               parentTypeId="application">
            <subapplication
                  label="Customer"
                  icon="org.eclipse.riena.demo.client:/icons/0457_a_a00.png"
                  typeId="org.eclipse.riena.demo.client.customer.customer"
                  view="customer"/>
         </assembly>
         <assembly
               id="org.eclipse.riena.demo.client.customer.mail"
               name="pim subApp"
               autostartsequence="200"
               assembler="org.eclipse.riena.demo.client.customer.navigation.model.PimSubApplicationNodeBuilder"
               parentTypeId="application">
         </assembly>
      
         <assembly
            id="org.eclipse.riena.demo.client.kundensuche"
            autostartsequence="110"
            name="kundensuche"
            parentTypeId="org.eclipse.riena.demo.client.customer.customer">
         <modulegroup
               name="kundesuche_group"
               typeId="org.eclipse.riena.demo.client.kundensuche.group">
            <module
                  name="kundesuche"
                  label="Kundensuche">
               <submodule
                     typeId="org.eclipse.riena.demo.client.kundensuche"
                     name="kundensuche"
                     label="Suche"
                     icon="org.eclipse.riena.demo.client:/icons/ledlightgreen.png"
                     view="org.eclipse.riena.demo.kundensuche"
                     controller="org.eclipse.riena.demo.client.customer.controllers.CustomerSearchController"/>
            </module>
         </modulegroup>
      </assembly>
      <assembly
            id="org.eclipse.riena.demo.client.customerRecord"
            name="customerRecord"
            parentTypeId="org.eclipse.riena.demo.client.customer.customer">
         <modulegroup
               typeId="customerRecordGroup">
            <module
                  label="${riena.navigation.parameter:firstName} ${riena.navigation.parameter:lastName}"
                  typeId="org.eclipse.riena.demo.client.module.CustomerRecord">
               <submodule
                     controller="org.eclipse.riena.demo.client.customer.controllers.CustomerOverviewController"
                     label="Übersicht"
                     shared="false"
                     typeId="customerRecordOverview"
                     view="org.eclipse.riena.demo.customerRecordOverview">
               </submodule>
               <submodule
                     controller="org.eclipse.riena.demo.client.customer.controllers.CustomerPersonalDataController"
                     label="Personendaten"
                     shared="false"
                     typeId="customerRecordPersonalData"
                     view="org.eclipse.riena.demo.customerRecordPersonalData">
               </submodule>
               <submodule
                     controller="org.eclipse.riena.demo.client.customer.controllers.CustomerAddressController"
                     label="Adressdaten"
                     shared="false"
                     typeId="customerRecordAddress"
                     view="org.eclipse.riena.demo.customerRecordAddress">
               </submodule>
               <submodule
                     controller="org.eclipse.riena.demo.client.customer.controllers.CustomerContractController"
                     label="Verträge"
                     shared="false"
                     typeId="customerRecordContract"
                     view="org.eclipse.riena.demo.customerRecordContract">
               </submodule>
            </module>
         </modulegroup>
      </assembly>
      <assembly
            id="org.eclipse.riena.demo.client.customer.order"
            name="order subapp"
            autostartsequence="300"
            assembler="org.eclipse.riena.demo.client.customer.navigation.model.OrderSubApplicationNodeBuilder"
            parentTypeId="application">
      </assembly>
     </extension>

</plugin>
