<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<!-- @deprecated -->
	<extension-point
		id="org.eclipse.riena.communication.hessian.AbstractSerializerFactory"
		name="Abstract Serializer Factory"
		schema="schema/org.eclipse.riena.communication.hessian.AbstractSerializerFactory.exsd" />

	<extension-point
		id="serializerFactories"
		name="Hessian Serializer Factories"
		schema="schema/serializerFactories.exsd" />
		
	<extension
		id="org.eclipse.riena.communication.hessian.PrincipalSerializerFactory"
		point="org.eclipse.riena.communication.factory.hessian.serializerFactories">
		<factory
			implementation="org.eclipse.riena.internal.communication.factory.hessian.PrincipalSerializerFactory">
		</factory>
	</extension>
	
	<extension
		id="org.eclipse.riena.communication.hessian.InputSerializerFactory"
		point="org.eclipse.riena.communication.factory.hessian.serializerFactories">
		<factory
			implementation="org.eclipse.riena.internal.communication.factory.hessian.InputStreamSerializerFactory">
		</factory>
	</extension>

	<extension
		id="org.eclipse.riena.communication.hessian.StacktraceElementSerializerFactory"
		point="org.eclipse.riena.communication.factory.hessian.serializerFactories">
		<factory
			implementation="org.eclipse.riena.internal.communication.factory.hessian.StacktraceElementSerializerFactory">
		</factory>
	</extension>
	
	<extension
		id="org.eclipse.riena.communication.hessian.GregorianCalendarSerializerFactory"
		point="org.eclipse.riena.communication.factory.hessian.serializerFactories">
		<factory
			implementation="org.eclipse.riena.internal.communication.factory.hessian.GregorianCalendarSerializerFactory">
		</factory>
	</extension>
	
	<extension
		id="org.eclipse.riena.communication.hessian.PermissionsSerializerFactory"
		point="org.eclipse.riena.communication.factory.hessian.serializerFactories">
		<factory
			implementation="org.eclipse.riena.internal.communication.factory.hessian.PermissionsSerializerFactory">
		</factory>
	</extension>

	<extension
		id="org.eclipse.riena.communication.hessian.JavaPermissionSerializerFactory"
		point="org.eclipse.riena.communication.factory.hessian.serializerFactories">
		<factory
			implementation="org.eclipse.riena.internal.communication.factory.hessian.JavaPermissionSerializerFactory">
		</factory>
	</extension>

	<extension
		point="org.eclipse.riena.communication.core.remoteServiceFactories">
		<factory
			protocol="hessian"
			remoteServiceFactory="org.eclipse.riena.internal.communication.factory.hessian.RemoteServiceFactoryHessian">
		</factory>
	</extension>

</plugin>
