<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="org.eclipse.riena.communication.hessian.AbstractSerializerFactory" name="Abstract Serializer Factory" schema="schema/org.eclipse.riena.communication.hessian.AbstractSerializerFactory.exsd"/>
   <extension
         id="org.eclipse.riena.communication.hessian.PrincipalSerializerFactory"
         point="org.eclipse.riena.communication.hessian.AbstractSerializerFactory">
      <factory
            implementation="org.eclipse.riena.internal.communication.factory.hessian.PrincipalSerializerFactory">
      </factory>
   </extension>
   <extension
         id="org.eclipse.riena.communication.hessian.InputSerializerFactory"
         point="org.eclipse.riena.communication.hessian.AbstractSerializerFactory">
      <factory
            implementation="org.eclipse.riena.internal.communication.factory.hessian.InputStreamSerializerFactory">
      </factory>
   </extension>

	<extension
        point="org.eclipse.riena.core.startups">
      <startup 
      	activateSelf="true">
      </startup>
   </extension>
 <extension
       id="org.eclipse.riena.communication.hessian.StacktraceElementSerializerFactory"
       point="org.eclipse.riena.communication.hessian.AbstractSerializerFactory">
    <factory
          implementation="org.eclipse.riena.internal.communication.factory.hessian.StacktraceElementSerializerFactory">
    </factory>
 </extension>

</plugin>
